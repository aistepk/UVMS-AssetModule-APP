<ejb-jar
	xmlns = "http://java.sun.com/xml/ns/javaee"
	version = "3.1"
	xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<interceptors>
		<interceptor>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor>
	</interceptors>
	
	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>AssetFilterServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>AssetConfigHelper</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>AssetServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>AuditServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>ConfigServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>CustomCodesServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>ConfigServiceBeanMT</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MobileTerminalEventServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>MobileTerminalServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>PluginServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>PollServiceBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>AssetDao</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
		<interceptor-binding>
			<ejb-name>TerminalDaoBean</ejb-name>
			<interceptor-class>net.bull.javamelody.MonitoringInterceptor</interceptor-class>
		</interceptor-binding>
	</assembly-descriptor>
</ejb-jar>